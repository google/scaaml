{
    "problemMatcher": [
        {
            "owner": "mdlint",
            "pattern": [
                {
                    "regexp": "^([^:]+):(\\d+):(\\d+)\\s*(MD[^\\s]+)\\s+(.*)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "code": 4,
                    "message": 5
                }
            ]
        }
    ]
}
