{
    "problemMatcher": [
        {
            "owner": "spellcheck",
            "pattern": [
                {
                    "regexp": "^\\d*\\/\\d*\\s*\\.\\/([^\\s]+).*$",
                    "file": 1
                },
                {
                    "regexp": "^[^:]+:(\\d+):(\\d+)\\s*-\\s*(.*)$",
                    "line": 1,
                    "column": 2,
                    "message": 3
                }
            ]
        }
    ]
}
