{
    "problemMatcher": [
        {
            "owner": "spellcheck",
            "pattern": [
                {
                    "regexp": "^([^:]+):(\\d+):([^:]+)\\s*-\\s*(.*)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "message": 4
                }
            ]
        }
    ]
}
